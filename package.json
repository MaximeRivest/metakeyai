{
  "name": "metakeyai-app",
  "productName": "metakeyai-app",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "npm run setup:python && electron-forge start",
    "package": "npm run setup:python && npm run build:python && electron-forge package",
    "make": "npm run setup:python && npm run build:python && electron-forge make",
    "publish": "npm run setup:python && npm run build:python && electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "setup:python": "node scripts/setup-python.js",
    "setup:audio": "node scripts/setup-audio-binaries.js",
    "build:python": "node scripts/setup-windows-python.js && node scripts/setup-audio-binaries.js && node scripts/build-python.js",
    "clean:python": "rm -rf python-env",
    "dev": "electron-forge start",
    "release": "node scripts/create-release.js",
    "postinstall": "node scripts/setup-python.js || echo 'Python setup skipped'"
  },
  "keywords": [],
  "author": {
    "name": "Maxime Rivest",
    "email": "mrive052@gmail.com"
  },
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-webpack": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@reforged/maker-appimage": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^6.11.0",
    "electron": "36.5.0",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.31.0",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "node-loader": "^2.1.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "dotenv": "^16.5.0",
    "electron-log": "^5.4.1",
    "electron-squirrel-startup": "^1.0.1",
    "form-data": "^4.0.3",
    "unzipper": "^0.10.11"
  }
}
